    <script type="text/javascript">
        function getUrlParameter(sParam) {
            var sPageURL = window.location.search.substring(1);
            var sURLVariables = sPageURL.split('&');
            for (var i = 0; i < sURLVariables.length; i++) {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == sParam) {
                    return sParameterName[1];
                }
            }
        }
    </script>
    <script type="text/javascript">
        function hidePowerdByGoogle() {

            $(".search-input input").attr("style", "background:none;");
            $(".search-input input").val("");

        }

        function showPowerdByGoogle() {

            if ($(".search-input input").val().trim() == "" || $(".search-input input").val().trim() == "Cerca") {
                $(".search-input input").removeAttr("style");
                $(".search-input input").val("Cerca");
            }

        }

        function closeButtonClick() {
            $(".search-input input").val("");
            $(".close-button").hide();

            showPowerdByGoogle();
        }

        function closeButtonShow() {
            $(".close-button").show();
        }

        function inputKeyUp() {

            if ($(".search-input input").val().trim() == "" || (".search-input input").val().trim() == "Cerca") {
                $(".close-button").hide();
            } else {
                $(".close-button").show();
                hidePowerdByGoogle();
            }

        }

        function searchButtonClick(event) {
            if ($(".search-input input").val().trim() != "" && $(".search-input input").val().trim() != "Cerca") {
                $("#google-search-form").submit();
            }
        }


        $(document).ready(function () {
            if ($(".search-input input").val().trim() != "") {
                hidePowerdByGoogle();
                closeButtonShow();
            } else {
                $(".search-input input").val("Cerca");
            }
        });

    </script>
    <!-- La sezione di javascript che segue deve essere riportata prima delle chiamate ai javascript dei banner (in fondo al presente file di testo) -->
    <script type='text/javascript'>
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        (function () {
            var gads = document.createElement('script');
            gads.async = true;
            gads.type = 'text/javascript';
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') +
                    '//www.googletagservices.com/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);
        })();
    </script>

    <script type='text/javascript'>
        googletag.cmd.push(function () {

            var slots = {
                '/129279927/DFP_1019': 'div-gpt-ad-1465985834842-0'
            }

            if (cookie_consenso == undefined || cookie_consenso == "") {
                googletag.pubads().setCookieOptions(1);
            } else if (cookie_consenso.flag_cons == "NO") {
                googletag.pubads().setCookieOptions(1);
            }
            for (var slot in slots) {
                var myslot = googletag.defineSlot(slot, [[300, 250], [970, 250]], slots[slot]).addService(googletag.pubads());
            }

            googletag.pubads().addEventListener('slotRenderEnded', function (data) {

                if (data.isEmpty)
                    return;

                jQuery('#' + slots[data.slot.getAdUnitPath()] + ' iframe[width]').contents().click(onAgreemntDelegate);

            });

            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });

        function refreshAdvSlot() {
            googletag.pubads().setCookieOptions(0);
            googletag.pubads().refresh();
        }
    </script>

